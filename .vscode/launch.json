{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "preLaunchCommands": [],
            "request": "launch",
            "name": "Debug (OpenOCD)",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            "executable": "Build\\CoreA.ino.elf",
            "device": "STM32F103RCT6",
            "configFiles": [
                "interface/stlink-v2.cfg",
                "target/stm32f1x.cfg"
            ],
            "svdFile": "${workspaceRoot}/.vscode/STM32F103.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 8000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        }
    ]
}